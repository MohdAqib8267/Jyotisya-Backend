{
  "name": "jyotisya-ai",
  "version": "1.0.0",
  "description": "Jyotisya-ai backend",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "prisma": {
    "schema": "src/data/prisma/schema.prisma"
  },
  "scripts": {
    "copyfiles": "rsync -rP --exclude='*.ts' --include='*.sh' src/* dist/",
    "clean": "rm -rf dist/",
    "watch": "tsc -w",
    "prewatch": "yarn clean",
    "prebuild": "yarn clean",
    "build": "./node_modules/.bin/tsc",
    "postbuild": "yarn copyfiles && npx prisma generate && chmod +x ./src/scripts/**/*.sh",
    "dev": "tsc-watch --onSuccess \"node --es-module-specifier-resolution=node ./dist/server.js\""
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "prisma": "^4.13.0",
    "tsc-watch": "^6.0.0",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.315.0",
    "@aws-sdk/s3-request-presigner": "^3.315.0",
    "@google-cloud/local-auth": "2.1.0",
    "@paralleldrive/cuid2": "^2.2.0",
    "@prisma/client": "^4.13.0",
    "@types/cors": "^2.8.13",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/passport": "^1.0.11",
    "amqplib": "^0.10.3",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "105",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mysql": "^2.18.1",
    "nanoid": "3.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "qs": "^6.11.2",
    "razorpay": "^2.8.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
